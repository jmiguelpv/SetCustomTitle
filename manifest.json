{
    "name": "Rename Tabs",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "storage",
        "nativeMessaging",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
      "https://*/*",
      "http://*/*"
    ]
}
